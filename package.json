{
  "name": "@tencent/excel-diff-merge",
  "version": "1.1.2",
  "main": "./build/index.js",
  "typings": "./build/index.d.ts",
  "module": "dist/excel-diff-merge.esm.js",
  "browser": "dist/excel-diff-merge.umd.js",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^6.0.0",
    "rimraf": "^3.0.2",
    "xlsx": "^0.17.0"
  },
  "scripts": {
    "clean": "rimraf build",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "yarn prettier && yarn lint:src",
    "lint:fix": "yarn prettier --write && yarn lint:src:fix",
    "prettier": "prettier --check \"./**/*.{ts,tsx,js,json,jsx,scss,html,yaml,yml}\"",
    "lint:src": "yarn tslint && yarn eslint-check && yarn eslint",
    "lint:src:fix": "yarn tslint --fix && yarn eslint --fix",
    "tslint": "tslint -p .",
    "eslint": "eslint --cache \"./{src,typings,test}/**/*.{j,t}s{,x}\"",
    "eslint-check": "eslint-config-prettier .eslintrc.*",
    "build": "gulp build",
    "build-ts": "npm run clean && tsc -p ./tsconfig2.json",
    "release": "npm run build && npm publish",
    "release-ts": "npm run build-ts && npm publish",
    "dts-bundle": "yarn build && dts-bundle --name excel-diff-merge --main dist/index.d.ts --out index.d.ts",
    "prepare": "husky install && echo 'PATH=$PATH:'$PATH >> .husky/_/husky.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^26.6.3",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "delete": "^1.1.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "tsify": "^5.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.1.2",
    "typescript-tslint-plugin": "^1.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
